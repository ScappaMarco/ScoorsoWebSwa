{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://swa.net/schemas/request",
    "type": "object",
    "properties": {
        "id": {
            "type":"intteger",
            "minimum": 1
        },
        "updateText": {
            "type":"string"
        },
        "immissionTime": {
            "type":"string",
            "format":"date-time"
        },
        "admin": {
            "type":"/schemas/admin"
        },
        "mission": {
            "$ref":"/schemas/mission"
        }
    },
    "required": ["updateText", "immissionTime", "admin", "mission"],
    "additionalProperties":false
}