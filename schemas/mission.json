{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://swa.net/schemas/request",
    "type": "object",
    "properties": {
        "id": {
            "type":"intteger",
            "minimum": 1
        },
        "teamLeader": {
            "$ref":"/schemas(user"
        },
        "team": {
            "type":"array",
            "itmes": {
                "$ref":"/schema/user"
            }
        },
        "objective": {
            "type":"string"
        },
        "position": {
            "type": "object",
            "properties": {
                "address": {
                    "type":"string"
                },
                "coordinates": {
                    "type":"string"
                }
            }
        },
        "startTime": {
            "type":"string",
            "format":"date-time"
        }
    }
}