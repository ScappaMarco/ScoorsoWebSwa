{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://swa.net/schemas/request",
    "type": "object",
    "properties": {
        "id": {
            "type":"intteger",
            "minimum": 1
        },
        "teamLeader": {
            "$ref":"/schemas/operator"
        },
        "team": {
            "type":"array",
            "itmes": {
                "$ref":"/schema/operator"
            }
        },
        "objective": {
            "type":"string"
        },
        "startTime": {
            "type":"string",
            "format":"date-time"
        }, 
        "endTime": {
            "type":"string",
            "format":"date-time"
        },
        "successLevel": {
            "type":"integer",
            "minimum":0,
            "maximum":5
        }
    },
    "required": ["teamLeader", "team", "objective", "position", "startTime"],
    "additionalItems":false
}